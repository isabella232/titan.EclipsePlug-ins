module magic_number {
control {
  // 6, 6, 43, 123... are magic numbers
  var integer i := 6;
  for (i := -6; i< -43; i := i * 1234567898765432123456789) {}
  
  // 2345, 5, 0 are not magic numbers
  const integer c := -2345;  
  i := 5 * (-5) + 0;
  
}
}