module nonprivate {
// these definitions are local, should be private
altstep al() {
  [] any port.receive {}
}
function fl() {}
type integer tyl;
template integer tel := *;
const integer cl := 0;
external function efl();
external const integer ecl;

// these are non-local
altstep an() {
  [] any port.receive {}
}
function fn() {}
type integer tyn;
template integer ten := *;
const integer cn := 0;
external function efn();
external const integer ecn;


control {
  fl();
  var tyl il := cl + ecl;
  if (match(il, tel)) {
    efl();
  } else {
    activate(al());
  }
}
}